#PROXY-START/

location ~ ^/(api|auth|repositories|quotes|stats|endpoints)/ {
    proxy_pass http://43.130.230.249:3077;
    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
}

location /
{
    proxy_pass http://43.130.230.249:3088;
    proxy_set_header Host 43.130.230.249;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header REMOTE-HOST $remote_addr;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection $connection_upgrade;
    proxy_http_version 1.1;

    add_header X-Cache $upstream_cache_status;

    set $static_filecNFp86Li 0;
    if ( $uri ~* "\.(gif|png|jpg|css|js|woff|woff2)$" )
    {
        set $static_filecNFp86Li 1;
        expires 1m;
    }
    if ( $static_filecNFp86Li = 0 )
    {
        add_header Cache-Control no-cache;
    }
}
#PROXY-END/